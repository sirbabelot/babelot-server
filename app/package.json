    "app-module-path": "^1.0.6",
    "bluebird": "3.3.1",
    "body-parser": "1.15.0",
    "cors": "2.7.1",
    "expect": "1.14.0",
    "express": "^4.13.4",
    "express-jwt": "3.3.0",
